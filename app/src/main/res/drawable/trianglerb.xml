<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <rotate
            android:fromDegrees="-45"
            android:toDegrees="0"
            android:pivotX="50%"
            android:pivotY="100%"
            >
            <shape android:shape="rectangle" >
                <stroke android:color="#00467E" android:width="10dp"/>
                <solid android:color="#00467E" />
            </shape>
        </rotate>
    </item>
</layer-list>